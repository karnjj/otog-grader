c:
  compile: gcc [sourcePath] -O2 -std=c17 -fomit-frame-pointer -Wunused-result -o env/out 2>env/error.txt
  execute: env/out[inputfile]
  extension: c
cpp:
  compile: g++ [sourcePath] -O2 -std=c++17 -fomit-frame-pointer -Wunused-result -o env/out 2>env/error.txt
  execute: env/out[inputfile]
  extension: cpp
python:
  compile: python3 -m py_compile [sourcePath] 2>env/error.txt
  execute: python3 [sourcePath][inputfile]
  extension: py
